{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "requirePragma": false,
  "jsxBracketSameLine": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 80,
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "trailingComma": "all",
  "importOrder": [
    "^[./]",
    "./styles",
    "@/routes",
    "@/typings",
    "@/pages",
    "@/store",
    "@/assets|@/component",
    "@/hooks",
    "@/services",
    "@/common",
    "@/api-graphql"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
