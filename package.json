{
  "name": "finder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format:check": "prettier --check src",
    "format:fix": "prettier --write src ",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:fix": "yarn lint --fix",
    "gen": "pmp -o src/api-graphql.ts --codegen-functions --codegen-react-hooks",
    "gen:luxury": "node node_modules/generate-graphql-api/dist/index.js -o ./src/api-graphql.ts --codegen-functions --codegen-react-hooks",
    "prepare": "husky install  && chmod ug+x .husky/*"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@giphy/react-components": "^6.2.0",
    "@lottiefiles/react-lottie-player": "^3.4.7",
    "@types/zen-observable": "^0.8.3",
    "apollo-upload-client": "^17.0.0",
    "array-move": "^3.0.1",
    "axios": "^1.2.0",
    "emoji-picker-react": "^4.4.5",
    "formik": "^2.2.9",
    "framer-motion": "^7.6.2",
    "generate-graphql-api": "^2.0.1",
    "graphql": "^16.6.0",
    "html2canvas": "^1.4.1",
    "husky": ">=6",
    "i18next": "^22.0.2",
    "i18next-browser-languagedetector": "^6.1.8",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-easy-sort": "^1.5.1",
    "react-geolocated": "^4.0.3",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.6.0",
    "react-range": "^1.8.14",
    "react-router-dom": "^6.4.2",
    "react-social-login-buttons": "^3.6.1",
    "react-toastify": "^9.1.1",
    "reactjs-social-login": "^2.6.1",
    "socket.io-client": "^4.5.4",
    "styled-components": "^5.3.6",
    "twin.macro": "rc",
    "usehooks-ts": "^2.9.1",
    "uuid": "^9.0.0",
    "yup": "^0.32.11",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": ">=10",
    "postcss": "^8.4.18",
    "prettier": "^2.8.0",
    "tailwindcss": "^3.2.3",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.cjs",
      "preset": "styled-components"
    }
  }
